<!DOCTYPE UI><UI version="3.1" stdsetdef="1">
<class>WordListUI</class>
<widget class="QWidget">
    <property name="name">
        <cstring>WordListUI</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>323</width>
            <height>111</height>
        </rect>
    </property>
    <property name="caption">
        <string>Form1</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QLabel" row="0" column="0">
            <property name="name">
                <cstring>languageLabel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>5</hsizetype>
                    <vsizetype>0</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>&amp;Language code:</string>
            </property>
            <property name="buddy" stdset="0">
                <cstring>languageEdit</cstring>
            </property>
        </widget>
        <widget class="QCheckBox" row="1" column="0" rowspan="1" colspan="2">
            <property name="name">
                <cstring>dictionaryCheckBox</cstring>
            </property>
            <property name="text">
                <string>compare to OpenOffice.org dictionary:</string>
            </property>
        </widget>
        <widget class="QLineEdit" row="0" column="1">
            <property name="name">
                <cstring>languageEdit</cstring>
            </property>
            <property name="text">
                <string></string>
            </property>
        </widget>
        <widget class="KURLRequester" row="2" column="1">
            <property name="name">
                <cstring>dictionaryURL</cstring>
            </property>
            <property name="enabled">
                <bool>false</bool>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>5</hsizetype>
                    <vsizetype>0</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
        </widget>
    </grid>
</widget>
<connections>
    <connection>
        <sender>dictionaryCheckBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>dictionaryURL</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
<layoutdefaults spacing="6" margin="11"/>
<includehints>
    <includehint>kurlrequester.h</includehint>
    <includehint>klineedit.h</includehint>
    <includehint>kpushbutton.h</includehint>
</includehints>
</UI>
